#include <stdio.h>
int main()
{
    long int M,N,X,Y,T,i;
    scanf("%ld",&T);
    long int arr[T];
    for(i=0;i<T;i++)
        arr[i]=0;
    for(i=0;i<T;i++)
    {
        scanf("%ld %ld %ld %ld",&M,&N,&X,&Y);
        long int l=0,u=N,m,temp;
        while(1)
        {
            m=(l+u)/2;
            temp=M;
            temp=temp+(N-m)*Y;
            if(X*m<=temp)
            {
                l++;
                arr[i]=m;
            }
            else
               u--;
            if(l>u)
                break;
        }
    }
    for(i=0;i<T;i++)
            printf("%ld\n",arr[i]);
}
