#include <stdio.h>
int main()
{
    long int M,N,X,Y,T,i,j=0,n=0;
    scanf("%ld",&T);
    long int arr[T];
    for(i=0;i<T;i++)
    {
        scanf("%ld %ld %ld %ld",&M,&N,&X,&Y);
        while(N>0)
        {
            if(X*N<=M)
            {
                arr[j]=N;
                j++;
                n++;
                break;
            }
            else
            {
                N--;
                M=M+Y;
            }
            if(N==0)
            { 
                arr[j]=0;
                j++;
                n++;
            }
        }
    }
    for(i=0;i<n;i++)
            printf("%ld\n",arr[i]);
}
